{
  "name": "api-arv-controll",
  "version": "0.2.123",
  "main": "dist/server.js",
  "type": "commonjs",
  "license": "MIT",
  "scripts": {
    "build": "tsc --build",
    "build:clean": "tsc --build --clean",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "prepare": "husky install",
    "start:dev": "ts-node-dev src/server.ts",
    "start:prd": "node dist/server.js",
    "test": "jest --passWithNoTests --silent --runInBand",
    "test:verbose": "jest --passWithNoTests",
    "test:unit": "npm run test -- --watch",
    "test:coverage": "jest --coverage",
    "release": "standard-version"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/mysql": "^2.15.20",
    "@types/node": "^17.0.25",
    "@types/triple-beam": "^1.3.2",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "eslint": "^8.13.0",
    "eslint-config-google": "^0.14.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.8",
    "prettier": "^2.6.2",
    "standard-version": "^9.3.2",
    "ts-jest": "^27.1.4",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "express": "^4.17.2",
    "git-commit-msg-linter": "^4.1.1",
    "moment": "^2.29.1",
    "mongoose": "^6.1.5",
    "triple-beam": "^1.3.0",
    "winston": "^3.4.0"
  }
}
