{
  "name": "api-arv-controll",
  "version": "1.1.0",
  "main": "dist/entrypoint/http/config/server.js",
  "type": "commonjs",
  "license": "MIT",
  "scripts": {
    "build": "rimraf ./dist && tsc --build",
    "build:clean": "tsc --build --clean",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "start:dev": "ts-node-dev src/entrypoint/http/config/server.ts",
    "start:prd": "npm run build && node dist/entrypoint/http/config/server.js",
    "test": "jest --passWithNoTests --silent --runInBand",
    "test:verbose": "jest --passWithNoTests",
    "test:unit": "npm run test -- --watch",
    "test:coverage": "jest --coverage",
    "release": "standard-version",
    "docker:build": "docker build --rm -f \"Dockerfile\" -t 741258321/arv-controll:latest \".\"",
    "docker:run": "docker run -d --name api-arv-controll --restart unless-stopped --env-file .env -p 5000:5000 741258321/arv-controll:latest"
  },
  "devDependencies": {
    "@types/cors": "2.8.12",
    "@types/dotenv": "8.2.0",
    "@types/express": "4.17.13",
    "@types/jest": "28.1.8",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "18.7.13",
    "@types/winston": "2.4.4",
    "@typescript-eslint/eslint-plugin": "5.20.0",
    "@typescript-eslint/parser": "5.20.0",
    "eslint": "8.13.0",
    "eslint-plugin-prettier": "4.0.0",
    "git-commit-msg-linter": "4.1.3",
    "husky": "7.0.4",
    "jest": "28.1.3",
    "lint-staged": "12.3.8",
    "prettier": "2.7.1",
    "ts-jest": "28.0.8",
    "ts-node-dev": "2.0.0",
    "typescript": "4.8.2"
  },
  "dependencies": {
    "@types/pg": "^8.6.5",
    "cors": "2.8.5",
    "dotenv": "16.0.1",
    "elastic-apm-node": "3.38.0",
    "express": "4.18.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "2.29.1",
    "mongoose": "6.4.1",
    "pg": "^8.8.0",
    "winston": "3.8.0"
  }
}
